{
  "dashboard": {
    "title": "ORCA AI-Agent Mesh - E2E Observability",
    "tags": ["orca", "e2e", "observability", "metrics"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "row",
        "title": "E2E Test Metrics",
        "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
        "collapsed": false
      },
      {
        "id": 2,
        "type": "stat",
        "title": "E2E Test Pass Rate",
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 1 },
        "targets": [
          {
            "expr": "(sum(rate(e2e_test_passed_total[5m])) / sum(rate(e2e_test_total[5m]))) * 100",
            "refId": "A",
            "legendFormat": "Pass Rate"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 80, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "E2E Test Success Count",
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 1 },
        "targets": [
          {
            "expr": "sum(increase(e2e_test_passed_total[1h]))",
            "refId": "A",
            "legendFormat": "Passed Tests"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "E2E Test Failure Count",
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 1 },
        "targets": [
          {
            "expr": "sum(increase(e2e_test_failed_total[1h]))",
            "refId": "A",
            "legendFormat": "Failed Tests"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "yellow" },
                { "value": 5, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Average E2E Latency",
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 1 },
        "targets": [
          {
            "expr": "avg(e2e_test_duration_seconds)",
            "refId": "A",
            "legendFormat": "Avg Latency"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 5, "color": "yellow" },
                { "value": 10, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "type": "graph",
        "title": "E2E Test Duration Over Time",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(e2e_test_duration_seconds_bucket[5m])) by (le, test_name))",
            "refId": "A",
            "legendFormat": "p50 - {{test_name}}"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(e2e_test_duration_seconds_bucket[5m])) by (le, test_name))",
            "refId": "B",
            "legendFormat": "p95 - {{test_name}}"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(e2e_test_duration_seconds_bucket[5m])) by (le, test_name))",
            "refId": "C",
            "legendFormat": "p99 - {{test_name}}"
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration",
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 7,
        "type": "graph",
        "title": "E2E Test Pass/Fail Rate",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
        "targets": [
          {
            "expr": "sum(rate(e2e_test_passed_total[5m])) by (test_name)",
            "refId": "A",
            "legendFormat": "Passed - {{test_name}}"
          },
          {
            "expr": "sum(rate(e2e_test_failed_total[5m])) by (test_name)",
            "refId": "B",
            "legendFormat": "Failed - {{test_name}}"
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "yaxes": [
          {
            "format": "ops",
            "label": "Tests/sec",
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 8,
        "type": "row",
        "title": "API Performance Metrics",
        "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
        "collapsed": false
      },
      {
        "id": 9,
        "type": "graph",
        "title": "HTTP Request Rate",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (method, status)",
            "refId": "A",
            "legendFormat": "{{method}} {{status}}"
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec",
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 10,
        "type": "graph",
        "title": "HTTP Request Duration (p95)",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "refId": "A",
            "legendFormat": "p95 - {{endpoint}}"
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration",
            "show": true
          },
          {
            "show": true
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.7],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "HTTP Request Duration Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
