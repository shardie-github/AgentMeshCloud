{
  "name": "@ai-agent-mesh/sdk",
  "version": "3.0.0",
  "description": "Official JavaScript/TypeScript SDK for AI-Agent Mesh platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestration",
    "mcp",
    "mesh",
    "governance",
    "typescript"
  ],
  "author": "AI-Agent Mesh Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-agent-mesh/sdk-js.git"
  },
  "homepage": "https://docs.ai-agent-mesh.com/sdk/javascript",
  "bugs": {
    "url": "https://github.com/ai-agent-mesh/sdk-js/issues"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
